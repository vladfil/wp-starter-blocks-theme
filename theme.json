{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "740px",
      "wideSize": "1100px"
    },
    "color": {
      "palette": [
        {
          "color": "#E5E5E5",
          "slug": "beige",
          "name": "Beige"
        },
        {
          "color": "#928876",
          "slug": "bronze",
          "name": "Bronze"
        },
        {
          "color": "#FBF06D",
          "slug": "gold",
          "name": "Gold"
        },
        {
          "color": "#DFD5C4",
          "slug": "grey",
          "name": "Grey"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Outfit",
          "name": "Outfit",
          "slug": "outfit",
          "fontFace": [
            {
              "fontFamily": "Outfit Light",
              "fontStyle": "normal",
              "fontWeight": "300",
              "src": ["file:./assets/fonts/Outfit/Outfit-Light.ttf"]
            },
            {
              "fontFamily": "Outfit Regular",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/Outfit/Outfit-Regular.ttf"]
            },
            {
              "fontFamily": "Outfit Bold",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/Outfit/Outfit-Bold.ttf"]
            }
          ]
        },
        {
          "fontFamily": "Karla",
          "name": "Karla",
          "slug": "karla",
          "fontFace": [
            {
              "fontFamily": "Karla Light",
              "fontStyle": "normal",
              "fontWeight": "300",
              "src": ["file:./assets/fonts/Karla/Karla-Light.ttf"]
            },
            {
              "fontFamily": "Karla Regular",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/Karla/Karla-Regular.ttf"]
            },
            {
              "fontFamily": "Karla Bold",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/Karla/Karla-Bold.ttf"]
            }
          ]
        }
      ],
      "fontWeight": true,
      "fontSizes": [
        {
          "slug": "s",
          "size": "1rem",
          "name": "S",
          "fluid": true
        },
        {
          "slug": "m",
          "size": "1.25rem",
          "name": "M",
          "fluid": true
        },
        {
          "slug": "l",
          "size": "1.5rem",
          "name": "L",
          "fluid": true
        },
        {
          "slug": "xl",
          "size": "2.5rem",
          "name": "XL",
          "fluid": true
        },
        {
          "slug": "xxl",
          "size": "3.5rem",
          "name": "XXL",
          "fluid": true
        },
        {
          "slug": "xxxl",
          "size": "4.5rem",
          "name": "XXXL",
          "fluid": true
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--karla)",
      "fontSize": "var(--wp--preset--font-size--m)"
    },
    "color": {
      "text": "var(--wp--preset--color--beige)",
      "background": "var(--wp--preset--color--black)"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--outfit)"
        },
        "color": {
          "text": "var(--wp--preset--color--white)"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxxl)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxl)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xl)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--l)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--m)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--s)"
        }
      }
    },
    "blocks": {
      "wp-starter-blocks-theme/example-static": {
        "elements": {}
      }
    }
  }
}