{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wp-starter-blocks-theme/example-static",
	"version": "0.1.0",
	"title": "Example Static",
	"category": "theme",
	"icon": "superhero",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"image": {
			"type": "object",
			"source": "html",
			"selector": ".hero__image",
      "properties": {
        "id": {
          "type": "number",
					"source": "attribute",
					"attribute": "id",
					"selector": ".hero__image"
        },
        "url": {
          "type": "string",
					"source": "attribute",
					"attribute": "src",
					"selector": ".hero__image"
        },
				"size": {
					"enum": ["full", "large", "middle", "small"],
					"default": "large",
					"source": "attribute",
					"attribute": "data-size",
					"selector": ".hero__image"
				},
				"alt": {
					"type": "string",
					"source": "attribute",
					"attribute": "alt",
					"selector": ".hero__image"
				}
      },
			"default": {
				"id": 0,
				"url": "",
        "size": "large",
        "alt": ""
			}
		},
		"title": {
      "type": "string",
      "default": "Example Static",
			"source": "text",
			"selector": "h2"
    },
		"subtitle": {
			"type": "string",
			"default": "Subtitle for Example Static",
			"source": "text",
			"selector": "h4"
		},
		"description": {
			"type": "string",
			"default": "Description for Example Static",
			"source": "text",
			"selector": "p"
		}
	},
	"textdomain": "example-static",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}